#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/project/unplugin-auto-img/node_modules/.pnpm/rollup@3.5.1/node_modules/rollup/dist/bin/node_modules:/mnt/d/project/unplugin-auto-img/node_modules/.pnpm/rollup@3.5.1/node_modules/rollup/dist/node_modules:/mnt/d/project/unplugin-auto-img/node_modules/.pnpm/rollup@3.5.1/node_modules/rollup/node_modules:/mnt/d/project/unplugin-auto-img/node_modules/.pnpm/rollup@3.5.1/node_modules:/mnt/d/project/unplugin-auto-img/node_modules/.pnpm/node_modules:/mnt/d/project/unplugin-auto-img/node_modules:/mnt/d/project/node_modules:/mnt/d/node_modules:/mnt/d/project/unplugin-auto-img/node_modules/.pnpm/tsup@6.5.0_typescript@4.9.3/node_modules/tsup/node_modules:/mnt/d/project/unplugin-auto-img/node_modules/.pnpm/tsup@6.5.0_typescript@4.9.3/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/project/unplugin-auto-img/node_modules/.pnpm/rollup@3.5.1/node_modules/rollup/dist/bin/node_modules:/mnt/d/project/unplugin-auto-img/node_modules/.pnpm/rollup@3.5.1/node_modules/rollup/dist/node_modules:/mnt/d/project/unplugin-auto-img/node_modules/.pnpm/rollup@3.5.1/node_modules/rollup/node_modules:/mnt/d/project/unplugin-auto-img/node_modules/.pnpm/rollup@3.5.1/node_modules:/mnt/d/project/unplugin-auto-img/node_modules/.pnpm/node_modules:/mnt/d/project/unplugin-auto-img/node_modules:/mnt/d/project/node_modules:/mnt/d/node_modules:/mnt/d/project/unplugin-auto-img/node_modules/.pnpm/tsup@6.5.0_typescript@4.9.3/node_modules/tsup/node_modules:/mnt/d/project/unplugin-auto-img/node_modules/.pnpm/tsup@6.5.0_typescript@4.9.3/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../rollup@3.5.1/node_modules/rollup/dist/bin/rollup" "$@"
else
  exec node  "$basedir/../../../../../rollup@3.5.1/node_modules/rollup/dist/bin/rollup" "$@"
fi
